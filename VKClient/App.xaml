                        Margin="0 21 0 20">
                <TextBlock Text="{Binding name}"
                           Margin="0 0 0 0"
                           FontSize="43"
                           FontFamily="{StaticResource PhoneFontFamilyLight}" />
            </StackPanel>

        </DataTemplate>

        <DataTemplate x:Key="SimplePickableItemTemplate">
            <StackPanel Orientation="Horizontal"
