<UserControl x:Class="VKClient.Common.UC.EarlierRepliesUC"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">
    
 
        <Grid Name="gridViewedFeedback" Height="48" Width="480" Background="{StaticResource PhoneNewsBackgroundBrush}">
          

            <TextBlock FontFamily="Segoe WP Semibold"
                       VerticalAlignment="Center"
                       Margin="24 0 0 0"
                       Text="{Binding LocalizedResources.ViewedFeedback, Source={StaticResource LocalizedStrings}}"
                       Foreground="{StaticResource PhoneVKSubtleBrush}"/>
        </Grid>
 
</UserControl>